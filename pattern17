import java.util.*;
public class Main 
{
	public static void main(String [] args)
	{
      Scanner s = new Scanner(System.in);
      int rows=s.nextInt();
      int a=1;
      for(int i=1;i<=rows;i++)
      {
          for(int j=1;j<=i;j++)
          {
              if(i==j)
              System.out.print(a);
              else
              System.out.print(a+" ");
              a++;
          }
          System.out.println();
      }
      s.close();
	}
}
