import java .util.*;
public class Main 
{
	public static void main(String [] args)
	{
        Scanner s=new Scanner(System.in);
        int rows=s.nextInt();
        for(int i=rows;i>=1;i--)
        {
            for(int j=i;j<=rows;j++)
            {
                if(j==rows)
                System.out.print(j);
                else
                System.out.print(j+" ");
            }
            System.out.println();
        }
        s.close();
	}    
}
