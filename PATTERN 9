import java.util.*;
public class Main 
{
	public static void main(String [] args)
	{
	    Scanner s = new Scanner(System.in);
	    int temp;
	    int a=s.nextInt();
        temp=a;
        for(int i=a;i>=1;i--)
        {
            for(int j=1;j<=i;j++)
            {
                if(j==temp)
                System.out.print(i);
                else
                System.out.print(i+" ");
            }
            temp--;
            System.out.println();
        }
	}
}
